meta {
  name: Get the logs for the build container or a service container for a given step of a pipeline.
  type: http
  seq: 15
}

get {
  url: {{baseUrl}}/repositories/:workspace/:repo_slug/pipelines/:pipeline_uuid/steps/:step_uuid/logs/:log_uuid
  body: none
  auth: none
}

params:path {
  workspace: 
  repo_slug: 
  pipeline_uuid: 
  step_uuid: 
  log_uuid: 
}
