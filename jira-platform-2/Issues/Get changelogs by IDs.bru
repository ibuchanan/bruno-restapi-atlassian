meta {
  name: Get changelogs by IDs
  type: http
  seq: 18
}

post {
  url: {{baseUrl}}/rest/api/2/issue/:issueIdOrKey/changelog/list
  body: json
  auth: basic
}

params:path {
  issueIdOrKey: {{issueKey}}
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

body:json {
  {
    "changelogIds": []
  }
}
