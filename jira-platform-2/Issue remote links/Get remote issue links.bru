meta {
  name: Get remote issue links
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/rest/api/2/issue/:issueIdOrKey/remotelink
  body: none
  auth: basic
}

params:query {
  ~globalId:
}

params:path {
  issueIdOrKey: {{issueKey}}
}

