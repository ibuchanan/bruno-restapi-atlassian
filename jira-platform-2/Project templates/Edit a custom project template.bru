meta {
  name: Edit a custom project template
  type: http
  seq: 2
}

put {
  url: {{baseUrl}}/rest/api/2/project-template/edit-template
  body: json
  auth: basic
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

body:json {
  {
    "templateDescription": "",
    "templateGenerationOptions": {
      "enableScreenDelegatedAdminSupport": false,
      "enableWorkflowDelegatedAdminSupport": false
    },
    "templateKey": "",
    "templateName": ""
  }
}
